{
    "compilerOptions": {
        "target": "es2020",
        "module": "esnext",
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "strict": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "types": [
            "@types/node",
            "@types/react"
        ]
    },
    "include": [
        "api/**/*"
    ]
}